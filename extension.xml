<extension xmlns="http://ns.adobe.com/air/extension/4.0">
    <id>com.heyzap.sdk.extensions.inmobi</id>
    <versionNumber>1.0</versionNumber>

    <platforms> 
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>InMobi.jar</nativeLibrary>
                <initializer>com.heyzap.sdk.extensions.inmobi</initializer>
            </applicationDeployment>
        </platform>

        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libInMobiANE.a</nativeLibrary>
                <initializer>InMobiANEInitializer</initializer>
                <finalizer>InMobiANEFinalizer</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="default"> 
            <applicationDeployment/> 
        </platform> 
    </platforms>
    
</extension>
